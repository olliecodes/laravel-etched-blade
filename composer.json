{
  "name": "olliecodes/laravel-etched",
  "description": "A markdown parser that uses Laravel blade to render content",
  "type": "library",
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/view": "^8.0",
    "league/commonmark": "^1.6",
    "webuni/front-matter": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^6.17"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Ollie Read",
      "email": "ollie@ollie.codes"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "OllieCodes\\Etched\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OllieCodes\\Etched\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  }
}
